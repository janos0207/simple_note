{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended"
        // "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-var": ["error"],
        "camelcase": ["error", {"ignoreDestructuring": true}],
        "quotes": ["error", "single"],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "eol-last": ["error"]
    }
}
